<launch>

<arg name="device" default="/dev/video4" />

<include file="$(find door_opening_utils)/launch/usb_cam.launch">
    <arg name="device" value="$(arg device)" />
</include>

<include file="$(find apriltag_ros)/launch/run_experiment.launch" />

<node name="arm_joint_vel_plot" pkg="rqt_multiplot" type="rqt_multiplot" args="--multiplot-config $(find door_opening_utils)/rqt_multiplot/arm_joint_velocities.xml" />
<node name="arm_command_plot" pkg="rqt_multiplot" type="rqt_multiplot" args="--multiplot-config $(find door_opening_utils)/rqt_multiplot/arm_commanded_position.xml" />

</launch>